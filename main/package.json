{
  "name": "nghacks",
  "version": "1.0.0",
  "description": "Useful reusable pieces built with Angular, Angular Material and some sort of Hack",
  "homepage": "https://ng-hack.web.app",
  "bugs": "https://github.com/abdunnahid/nghacks/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/abdunnahid/nghacks.git"
  },
  "author": {
    "name": "Abdun Nahid",
    "email": "abdun.nahid@gmail.com",
    "url": "https://abdun-nahid.web.app"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "npm run build:packages:prod && npm run build:app:prod",
    "build:app:prod": "ng build --prod",
    "build:packages:prod": "npm run build:prod:ngmat-table-query-reflector && npm run build:prod:dynamic-browser-title && npm run build:prod:uploader && npm run build:prod:info-dialog && npm run build:prod:image-viewer && npm run build:prod:quantity-input && npm run build:prod:rating-input && npm run build:prod:banner-carousel && npm run build:prod:overflow-carousel",
    "serve:ngmat-table-query-reflector": "ng build --project=ngmat-table-query-reflector --watch",
    "build:prod:ngmat-table-query-reflector": "ng build --project=ngmat-table-query-reflector --prod",
    "serve:dynamic-browser-title": "ng build --project=dynamic-browser-title --watch",
    "build:prod:dynamic-browser-title": "ng build --project=dynamic-browser-title --prod",
    "serve:uploader": "ng build --project=uploader --watch",
    "build:prod:uploader": "ng build --project=uploader --prod",
    "serve:info-dialog": "ng build --project=info-dialog --watch",
    "build:prod:info-dialog": "ng build --project=info-dialog --prod",
    "serve:image-viewer": "ng build --project=image-viewer --watch",
    "build:prod:image-viewer": "ng build --project=image-viewer --prod",
    "serve:quantity-input": "ng build --project=quantity-input --watch",
    "build:prod:quantity-input": "ng build --project=quantity-input --prod",
    "serve:rating-input": "ng build --project=rating-input --watch",
    "build:prod:rating-input": "ng build --project=rating-input --prod",
    "serve:banner-carousel": "ng build --project=banner-carousel --watch",
    "build:prod:banner-carousel": "ng build --project=banner-carousel --prod",
    "serve:overflow-carousel": "ng build --project=overflow-carousel --watch",
    "build:prod:overflow-carousel": "ng build --project=overflow-carousel --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.4",
    "@angular/cdk": "^10.1.1",
    "@angular/common": "~10.0.4",
    "@angular/compiler": "~10.0.4",
    "@angular/core": "~10.0.4",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.0.4",
    "@angular/material": "^10.1.1",
    "@angular/platform-browser": "~10.0.4",
    "@angular/platform-browser-dynamic": "~10.0.4",
    "@angular/router": "~10.0.4",
    "angular-resize-event": "^2.0.0",
    "ngx-markdown": "^10.1.0",
    "prismjs": "^1.21.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.3",
    "@angular-devkit/build-ng-packagr": "~0.1000.4",
    "@angular/cli": "~10.0.3",
    "@angular/compiler-cli": "~10.0.4",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
